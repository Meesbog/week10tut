{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "data": {"url": "top_six_economies.csv"},
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "year", "field": "Year", "title":null,
      "axis": {"domain": false, "format": "%Y", "tickSize": 0}
    },
    "y": {
      "aggregate": "sum", "field": "Total reserves (includes gold, current US$)"
    },
    "color": {"field": "Country Name", "type": "nominal", "legend": null, "scale": {"scheme": "set1"} }
  } }
    
  

